{
  "$schema": "http://json.schemastore.org/heatpump-capability",
  "type": "object",
  "properties": {
    "thermostat_mode": {
      "type": "string",
      "enum": [
        "off",
        "heat",
        "cool",
        "auto",
        "dry",
        "fan"
      ],
      "default": "off"
    }
  },
  "required": [
    "thermostat_mode"
  ],
  "additionalProperties": false,
  "insights": {
    "read": true,
    "write": true
  }
}