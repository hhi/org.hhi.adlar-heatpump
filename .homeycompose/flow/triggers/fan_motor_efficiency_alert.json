      "title": {
        "en": "Condition",
        "nl": "Conditie",
        "de": "Bedingung",
        "fr": "Condition"
      },
      "values": [
        {
          "id": "above",
          "title": {
            "en": "above",
            "nl": "boven",
            "de": "über",
            "fr": "au-dessus"
          }
        },
        {
          "id": "below",
          "title": {
            "en": "below",
            "nl": "onder",
            "de": "unter",
            "fr": "en dessous"
          }
        }
      ]
    },
    {
      "type": "range",
      "name": "frequency",
      "title": {
        "en": " Frequency ",
        "nl": " Frequentie ",
        "de": " Frequenz ",
