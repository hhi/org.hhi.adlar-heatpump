{
  "id": "compressor_efficiency_alert",
  "title": {
    "en": "Compressor efficiency alert",
    "nl": "Compressor efficiëntie alarm",
    "de": "Kompressor-Effizienzalarm",
    "fr": "Alerte d'efficacité du compresseur"
  },
  "titleFormatted": {
    "en": "Compressor frequency [[condition]] [[frequency]] Hz",
    "nl": "Compressor frequentie [[condition]] [[frequency]] Hz",
    "de": "Kompressor-Frequenz [[condition]] [[frequency]] Hz",
    "fr": "Fréquence du compresseur [[condition]] [[frequency]] Hz"
  },
  "hint": {
    "en": "Triggers when compressor frequency goes above or below a specified level",
    "nl": "Wordt geactiveerd wanneer compressor frequentie boven of onder een bepaald nivo gaat",
    "de": "Wird ausgelöst, wenn die Kompressor-Frequenz einen angegebenen Schwellenwert über- oder unterschreitet",
    "fr": "Se déclenche lorsque la fréquence du compresseur dépasse ou descend en dessous d'un niveau spécifié"
  },
  "args": [
    {
      "type": "device",
      "name": "device",
      "filter": "driver_id=intelligent-heat-pump"
    },
    {
      "type": "dropdown",
      "name": "condition",
      "title": {
        "en": "Condition",
        "nl": "Conditie"
      },
      "values": [
        {
          "id": "above",
          "title": {
            "en": "above",
            "nl": "boven"
          }
        },
        {
          "id": "below",
          "title": {
            "en": "below",
            "nl": "onder"
          }
        }
      ]
    },
    {
      "type": "range",
      "name": "frequency",
      "title": {
        "en": "Frequency (Hz)",
        "nl": "Frequentie (Hz)"
      },
      "min": 10,
      "max": 200,
      "step": 5,
      "units": "Hz",
      "placeholder": {
        "en": "Frequency threshold in Hz",
        "nl": "Frequentie drempel in Hz"
      }
    }
  ],
  "tokens": [
    {
      "type": "number",
      "name": "current_frequency",
      "title": {
        "en": "Current frequency",
        "nl": "Huidige frequentie",
        "de": "Aktuelle Frequenz",
        "fr": "Fréquence actuelle"
      },
      "example": 85
    },
    {
      "type": "number",
      "name": "threshold_frequency",
      "title": {
        "en": "Threshold frequency",
        "nl": "Drempel frequentie",
        "de": "Schwellenwert Frequenz",
        "fr": "Fréquence seuil"
      },
      "example": 90
    }
  ]
}